@model IEnumerable<Lucifron.ReST.Server.Models.ReadUserModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Prefix</th>
                <th>Pattern</th>
                <th>IPv4</th>
                <th>Token</th>
                <th>CredentialId</th>
            </tr>
        </thead>
        @foreach (var user in Model)
        {
        <tr>
            <td>
                @user.Id
            </td>
            <td>
                @user.Name
            </td>
            <td>
                @user.Prefix
            </td>
            <td>
                @user.Pattern
            </td>
            <td>
                @user.IPv4
            </td>
            <td>
                @user.Token
            </td>
            <td>
                @user.CredentialId
            </td>
        </tr>
        }
    </table>
</div>